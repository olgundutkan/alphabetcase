{
  "name": "@dutkan/alphabetcase",
  "version": "0.1.0",
  "description": "My first monorepo package",
  "scripts": {
    "bootstrap": "lerna bootstrap --force-local && lerna link --force-local",
    "exec": "lerna exec npm install",
    "compile": "lerna run --parallel compile",
    "clean": "lerna clean",
    "version": "lerna version --conventional-commits",
    "watch": "lerna run --parallel watch",
    "publish": "lerna publish --force-publish",
    "ls": "lerna ls"
  },
  "keywords": [
    "alphabetcase",
    "uppercase",
    "lowercase"
  ],
  "author": "olgundutkan <olgundutkan@gmail.com> (https://github.com/olgundutkan)",
  "license": "ISC",
  "devDependencies": {
    "lerna": "^3.22.1"
  }
}
